{
  "name": "pruny",
  "version": "1.0.11",
  "description": "Find and remove unused Next.js API routes",
  "type": "module",
  "bin": {
    "pruny": "./bin/pruny.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "bun build ./src/index.ts --outdir ./dist --target node",
    "dev": "bun run ./src/index.ts",
    "lint": "eslint src/**",
    "audit": "bun run build && node dist/index.js",
    "prepublishOnly": "bun run build"
  },
  "keywords": [
    "unused",
    "api",
    "routes",
    "nextjs",
    "cleanup",
    "lint"
  ],
  "author": "webnaresh",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/webnaresh/pruny"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "fast-glob": "^3.3.2",
    "minimatch": "^10.1.2"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@types/bun": "latest",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "eslint": "^10.0.0",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.54.0"
  }
}
